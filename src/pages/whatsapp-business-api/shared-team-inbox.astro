    const stickySection3 = document.querySelector(".shared-inbox-fixed-3");
    const stickySection4 = document.querySelector(".shared-inbox-fixed-4");
    const btnClassSection1 = document.querySelector(".faq-tab-wrapper");
    const btnClassSection2 = document.querySelector(
      ".brands-slider-container-partner"
    );

    // Create an Intersection Observer to track when .btn-class is visible or not
    const observer = new IntersectionObserver(
      (entries) => {
        entries.forEach((entry) => {
          if (entry.isIntersecting) {
            // If the .btn-class section is in view, hide the sticky section
            stickySection1.style.position = "relative";
            stickySection2.style.position = "relative";
            stickySection3.style.position = "relative";
            stickySection4.style.position = "relative";
          } else {
            // If the .btn-class section is out of view, show the sticky section
            stickySection1.style.position = "sticky";
            stickySection1.style.top = "90px";
            stickySection1.style.zIndex = "1";
            stickySection1.style.left = "0";
            stickySection2.style.position = "sticky";
            stickySection2.style.top = "90px";
            stickySection2.style.zIndex = "2";
            stickySection2.style.left = "0";
            stickySection3.style.position = "sticky";
            stickySection3.style.top = "90px";
            stickySection3.style.zIndex = "3";
            stickySection3.style.left = "0";
            stickySection4.style.position = "sticky";
            stickySection4.style.top = "0px";
            stickySection4.style.zIndex = "4";
            stickySection4.style.left = "0";
          }
        });
      },
      {
        threshold: 0, // Trigger when the element completely leaves the viewport
      }
    );

    // Observe the .btn-class section
    observer.observe(btnClassSection1, btnClassSection2);
  });
</script>

