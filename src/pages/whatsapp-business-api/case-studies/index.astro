---
import WhatsappLayout from "../../../layouts/whatsapp-business-api/WhatsappLayout.astro";
import "../../../../public/whatsapp-business-api/css/case-study.css";
import LogoStrips from "../../../components/LogoStrips.astro";
import PhoneApp from "../../../components/PhoneApp.astro";

const metaTitle = "WhatsApp Case Studies | Real Results with Emovur";
const metaDescription =
  "Explore real success stories of how Emovur helped brands grow using WhatsApp Business API across industries.";
const canonicalUrl = "https://emovur.com/whatsapp-business-api/case-studies";
const keywords =
  "digital marketing, emovur, whatsapp business, marketing services, online growth.";
---

<WhatsappLayout
  title={metaTitle}
  description={metaDescription}
  canonical={canonicalUrl}
  keywords={keywords}
>
  <!-- Landing form section ends -->
  <div class="case-study-main">
    <div
      class="container landing-form-section-container d-flex justify-content-between align-items-center"
    >
      <div
