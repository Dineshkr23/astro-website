---
import WhatsappLayout from "../../layouts/whatsapp-business-api/WhatsappLayout.astro";
import "../../../public/whatsapp-business-api/css/partner.css";
import LogoStrips from "../../components/LogoStrips.astro";
import PhoneApp from "../../components/PhoneApp.astro";

const metaTitle = "Become a WhatsApp API Partner | Emovur Partner Program";
const metaDescription =
  "Join Emovur’s partner program to resell WhatsApp Business API solutions and grow your digital services revenue.";
const canonicalUrl = "https://emovur.com/whatsapp-business-api/partner";
const keywords =
  "whatsapp api reseller, become a partner, agency partner program, emovur partners, reseller program.";
