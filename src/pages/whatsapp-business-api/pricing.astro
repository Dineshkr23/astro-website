                  document.getElementById("tooltip-workspace");
                const headPlans = document.getElementById("head-plans");

                if (tooltipWorkspace && headPlans) {
                  let timeoutId;

                  tooltipWorkspace.addEventListener("mouseover", function () {
                    // Cancelar cualquier timeout pendiente
                    if (timeoutId) {
                      clearTimeout(timeoutId);
                    }
                    // Aplicar z-index inmediatamente
                    headPlans.style.zIndex = "1";
                  });

                  tooltipWorkspace.addEventListener("mouseout", function () {
                    // Configurar un timeout de 1 segundo
                    timeoutId = setTimeout(function () {
                      headPlans.style.zIndex = "";
                    }, 500); // 1000 milisegundos = 1 segundo
                  });
                } else {
                  if (!tooltipWorkspace) {
                    console.warn(
                      "No se encontró el elemento #tooltip-workspace"
                    );
                  }
                  if (!headPlans) {
                    console.warn("No se encontró el elemento #head-plans");
                  }
