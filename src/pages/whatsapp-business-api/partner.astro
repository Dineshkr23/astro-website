                    placeholder="Enter your name"
                    required
                  /><br />
                  <label for="mobile_no">Phone Number</label><br />
                  <input
                    type="text"
                    id="mobile_no"
                    name="mobile_no"
                    placeholder="Your number goes here"
                    required
                  /><br />
                  <label for="partnership">Partnership</label><br />
                  <select id="partnership" name="partnership">
                    <option value="Select Partnership"
                      >Select Partnership</option
                    >
                    <option value="Affiliate">Affiliate</option>
                    <option value="WhiteLabel">WhiteLabel</option>
                  </select>
                  <button
                    type="submit"
                    class="pricing-cards-btn theme-btn signup-modal-button"
                  >
                    <i
                      class="fa fa-spinner fa-spin"
                      id="partner-form-loader"
                      style="display: none;"></i>
