  <script>
    function scrollToSection(sectionId) {
      var section = document.getElementById(sectionId);
      if (section) {
        var top = section.getBoundingClientRect().top;
        window.scrollTo({
          top: top - 100,
          behavior: "smooth",
        });
      }
    }

    window.onload = function () {
      var sectionId = window.location.hash.substring(1);
      if (sectionId) {
        scrollToSection(sectionId);
      }
    };
  </script>

  <script>
    window.addEventListener("scroll", function (event) {
      const mainWrapper = document.querySelector(".main-page-wrapper");
      if (window.scrollY < 100) {
        mainWrapper.style.overflowX = "hidden";
      } else {
        mainWrapper.style.overflowX = "clip";
      }
    });
  </script>

  <script>
    document.addEventListener("DOMContentLoaded", function () {
      document.querySelector(".first-image").classList.add("active");

      const content_container = document.querySelector(
        ".test-content-container"
      );
      content_container.addEventListener("scroll", function () {
        const containerHeight = content_container.offsetHeight;
        let windowHeight = window.innerHeight;
        let sections = document.querySelectorAll(".test-content-section");
        let images = document.querySelectorAll(".section-img");

        sections.forEach(function (section) {
          let sectionPosition = section.getBoundingClientRect().top;

          if (containerHeight - 100 > sectionPosition && sectionPosition > 0) {
            images.forEach(function (image) {
              image.classList.remove("active");
            });
            document
              .querySelector(`.${section.dataset.target}`)
              .classList.add("active");
          }
        });
      });

      document.addEventListener("scroll", function () {
        let windowHeight = window.innerHeight;
        const onboarding = document.querySelector(
          ".onboarding-process-container"
        );
        let onboardingPosition = onboarding.getBoundingClientRect().top;

        if (onboardingPosition < windowHeight) {
          document
            .querySelector(".test-heading")
            .classList.remove("sticky-heading");
        } else {
          document
            .querySelector(".test-heading")
            .classList.add("sticky-heading");
        }
      });

      window.dispatchEvent(new Event("scroll"));
    });
  </script>

