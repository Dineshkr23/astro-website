  <script>
    var names = [
      "automated",
      "ads",
      "shared",
      "broadcast",
      "analytics",
      "drip",
    ];
    var currentIndex = 0;

    function handleClick(event, name) {
      // Reset previous selections
      var elements = document.querySelectorAll(".blue-background, .active-svg");
      elements.forEach(function (element) {
        element.classList.remove("blue-background", "active-svg");
      });

      // Check if the clicked element is a descendant of features-points-container
      if (event.target.closest("#features-points-container")) {
        // Add the classes to the clicked element and its descendant SVG
        var clickedElement = event.target.closest(".features-points-container");
        var svgElement = clickedElement.querySelector(".svg-icon");
        clickedElement.classList.add("blue-background");
        svgElement.classList.add("active-svg");

        // Toggle visibility of additionalInfo
        var additionalInfos = document.querySelectorAll("#additionalInfo");
        additionalInfos.forEach(function (info) {
          info.style.display = "none";
        });

        var additionalInfo = clickedElement.querySelector("#additionalInfo");
        additionalInfo.style.display =
          additionalInfo.style.display === "none" ? "block" : "none";

        // var popup = document.getElementById('popup-image-container');
        var popup = document.getElementById("feature-image-container");
        // Replace the image source based on the name parameter
        var imageUrl;
        switch (name) {
          case "automated":
            imageUrl = "/images/1.png";
            break;
          case "ads":
            imageUrl = "/images/2.png";
            break;
          case "shared":
            imageUrl = "/images/3.png";
            break;
          case "broadcast":
            imageUrl = "/images/4.png";
            break;
          case "analytics":
            imageUrl = "/images/5.png";
            break;
          case "drip":
            imageUrl = "/images/6.png";
            break;
          default:
            imageUrl = "/images/1.png";
        }
        // popup.classList.remove('popup-image-container', 'automated', 'ads', 'shared', 'broadcast', 'analytics', 'drip');
        // popup.classList.add(name);
        // Set the new image source
        popup.innerHTML = '<img src="' + imageUrl + '" alt="Popup Image">';

        // Trigger the animation by adding the class
        popup.classList.add("animate-backInUp");

        // Remove the class after the animation duration
        setTimeout(function () {
          popup.classList.remove("animate-backInUp");
        }, 1000);
      }
    }

    function executeEveryThreeSeconds() {
      var name = names[currentIndex];

      currentIndex = (currentIndex + 1) % names.length;

      var clickedElement = document.querySelector(
        '.features-points-container[name="' + name + '"]'
      );

      if (clickedElement) {
        clickedElement.classList.add("blue-background");
        var svgElement = clickedElement.querySelector(".svg-icon");
        if (svgElement) {
          svgElement.classList.add("active-svg");
        }

        // Call handleClick with the simulated event and the current name
        handleClick(
          {
            target: clickedElement,
          },
          name
        );
      }
    }
    executeEveryThreeSeconds();

    setInterval(executeEveryThreeSeconds, 4000);
  </script>

