---
import Layout from "../../layouts/Layout.astro";
import "../../../public/css/service-pages/smm-style.css";

const metaTitle =
  "SEO Services | Dominate the first page of Google with Emovur";
const metaDescription =
  "Rank higher and stay ahead with Emovur’s expert SEO services. We optimize your website for visibility, traffic, and conversions—so you dominate the first page of Google.";
const canonicalUrl = "https://emovur.com/services/search-engine-optimization";
const keywords = "seo services, seo company, google seo.";
